    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "better-sqlite3": "^12.2.0",
    "drizzle-kit": "^0.20.0",
    "jest": "^30.0.5",
